<% @page_title = "Manage Questions" %>
<div class="center page-header">
  <h2>Manage Questions</h2>
  <%= link_to "Add new Question", new_question_path, :class => 'btn btn-primary' %>
</div>
<% @questions.each do |question| %>
    <div class="row">
      <div class="col-xs-8 col-xs-offset-2">
        <div class="well well-lg">
          <div class="program-title">
            <%= question.id %>
          </div>
          <div class="program-body">
            <%= link_to("Show", question_path(question), :class => 'btn btn-info') %>
            <%= link_to("Edit", edit_question_path(question), :class => 'btn btn-info') %>
            <%= link_to("Delete", delete_question_path(question), :class => 'btn btn-danger') %>
          </div>
        </div>
      </div>
    </div>
<% end %>
